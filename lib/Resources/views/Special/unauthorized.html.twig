{% extends ["::page.html.twig", "AppBundle:Include:page.html.twig", "AgitUiBundle:Include:page.html.twig"] %}

{% agit.capability "" %}
{% agit.status 401 %}
{% set noindex = 1 %}
{% agit.name t("Login required") %}

{% block js %}
    {% javascripts
        "@AgitUiBundle/Resources/public/js/lib/elem/Form.js"
        "@AgitUiBundle/Resources/public/js/lib/elem/FormRow.js"
        "@AgitUiBundle/Resources/public/js/lib/field/Password.js"

        "@AgitProfileBundle/Resources/public/js/lib/Form.js"
        "@AgitProfileBundle/Resources/public/js/var/translations.js"
        "@AgitProfileBundle/Resources/public/js/var/api.js"
        "@AgitProfileBundle/Resources/public/js/ctl/page.unauthorized.js"
        filter="?uglifyjs2"
    %}
    <script src="{{ asset_url }}" type="text/javascript"></script>
    {% endjavascripts %}
{% endblock %}

{% block templates %}
    {% include "AgitProfileBundle:Include:template.unauthorized.html.twig" %}
{% endblock %}
